namespace AdiçãoMarinaCarvalho
{
    class Calculator
    {
        static void Main(string[] args)
        {
            //Criar duas variáveis representando dois valores diferentes e soma-los
            int n1 = Convert.ToInt32(Console.ReadLine());
            int n2 = Convert.ToInt32(Console.ReadLine());
            int r = n1 + n2;
            Console.WriteLine("O valor de " + n1 + " + " + n2 + " é " + r);
        }
    }
}
